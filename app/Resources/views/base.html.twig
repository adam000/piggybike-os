                mixpanel.identify({{ app.user.id }});
                {% endif %}
                {% if app.session.get('MANUAL_REFERRER') is not empty %}
                    mixpanel.register_once({"Source": "{{ app.session.get('MANUAL_REFERRER') }}"});
