<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}{{ tagline }}{% endblock %}</title>
        <meta name="description" content="{{ description }}" />

        <meta name="google-site-verification" content="WmxttuUnBCQyKtH5YEnd8eOM8ls_GLKEWwm4ujAAnbU" />

        <meta name="og:title" content="{{ tagline }}"/>
        <meta name="og:description" content="{{ description }}"/>
        <meta name="og:image" content="{{ asset('images/app/images/fb-banner.png') }}" />
        <meta name="fb:app_id" content="{{ facebook_app_id }}"/>
        <meta name="og:url" content="http://piggy.bike/"/>

        <meta name="twitter:title" content="{{ tagline }}"/>
        <meta name="twitter:description" content="{{ description }}"/>
